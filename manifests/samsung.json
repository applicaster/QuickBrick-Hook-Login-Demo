  "general": {
    "fields": [
      {
        "type": "switch",
        "key": "is_flow_blocker",
        "tooltip_text": "Define if hook will block flow in case cancelation",
        "initial_value": true
      },
      {
        "type": "switch",
        "key": "allow_screen_plugin_presentation",
        "tooltip_text": "Define if",
        "initial_value": true
      }
