{
  "api": { "require_startup_execution": false, "modules": [] },
  "react_native": true,
  "dependency_repository_url": [],
  "dependency_name": "@applicaster/quick-brick-my-demo-login",
  "dependency_version": "0.0.1",
  "platform": "samsung_tv",
  "author_name": "Applicaster",
  "author_email": "zapp@applicaster.com",
  "manifest_version": "0.0.1",
  "name": "QuickBrick Login Hook",
  "description": "Login hook plugin for a demo",
  "type": "general",
  "screen": true,
  "identifier": "quick-brick-demo-login",
  "ui_builder_support": true,
  "whitelisted_account_ids": ["5ae06cef8fba0f00084bd3c6"],
